{
    "extends": "airbnb",
    "ignorePatterns": [
        "setupTests.js",
        "reportWebVitals.js",
        "index.js"
    ],
    "overrides": [
        {
            "files": [
                "*.js"
            ],
            "rules": {
                "react/no-array-index-key": "off",
                "react-hooks/exhaustive-deps": "off",
                "react/jsx-filename-extension": "off",
                "jsx-a11y/media-has-caption": "off"
            }
        }
    ]
}